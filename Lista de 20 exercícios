import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    
   Integer A;
   Integer B;
   
   Scanner entrada = new Scanner(System.in);
   
   System.out.println("Defina o valor inteiro de A:");
   B = entrada.nextInt();
   
   System.out.println("Agora, defina o valor inteiro de B:");
   A = entrada.nextInt();
    
   System.out.println("Portanto, o valores invertidos de A e B são  " + A + " e " + B + ", respectivamente.");
   
    
  }
}

