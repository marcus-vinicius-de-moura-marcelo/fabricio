  public static void main(String[] args) {
    
    Double dolar;
    Double real;
    
    Scanner entrada = new Scanner(System.in);
    
    System.out.println("Quantos dólares você possui?");
    dolar = entrada.nextDouble();
    
    real = dolar*4.95;
    System.out.println("Você possui essa quantidade, em reais: " + real);
    
      
  }
}

QUESTAO 3
import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    
    Double raio;
    Double pi;
    Double area;
    
    Scanner entrada = new Scanner(System.in);
    
    System.out.println("Digite o valor do raio de seu círculo, logo, será calculado a área dele.");
    raio = entrada.nextDouble();
    pi = 3.14;
    area = pi*(raio*raio);
    
    System.out.println("Este é o valor da área de seu círculo: " + area);
  }
}

QUESTAO 4
import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    
    Double nota1;
    Double nota2;
    Double nota3;
    Double media;
    
    Scanner entrada = new Scanner(System.in);
    
    System.out.println("Qual valor de sua primeira nota?");
    nota1 = entrada.nextDouble();
    
    System.out.println("Qual valor de sua segunda nota?");
    nota2 = entrada.nextDouble();
    
    System.out.println("Qual valor de sua terceira nota?");
    nota3 = entrada.nextDouble();
    
    media = (nota1 + nota2 + nota3)/3;
    
    System.out.println("Esta é a média de suas notas em suas três provas " + media);
    
  }
}

QUESTAO 5 
import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    
    Double bruto;
    Double valorHora;
    Double horas;
    Double inss;
    Double liquido;
    
    Scanner entrada = new Scanner(System.in);
    
   
    System.out.println("Qual valor da sua hora trabalhada?");
    valorHora = entrada.nextDouble();
    
    System.out.println("Quantas horas você trabalha por mês?");
    horas = entrada.nextDouble();
    
    bruto = horas*valorHora;
    
    System.out.println("Seu salário bruto é R$ " + bruto + ", " + "porém existe o desconto do INSS, que é de 10%" + '.');
    inss = bruto*0.1;
    liquido = bruto-inss;
   
    System.out.println("Portanto, seu salário líquido é R$ " + liquido);
    
  }
}

