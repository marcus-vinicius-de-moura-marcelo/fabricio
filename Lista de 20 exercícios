QUESTAO 8
import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    
    Double fahrenheit;
    Double celsius;  
    Double conversão;
    
    Scanner entrada = new Scanner(System.in);
    
    System.out.println("Quanto graus Celsius está fazendo em sua cidade?");
    celsius = entrada.nextDouble();
    conversão = 33.8;
    fahrenheit = celsius*conversão;
    
    
    System.out.println("Em Fahrenheit,a temperatura de sua cidade é de " + conversão + " graus.");
    
    
  }
}

QUESTAO 9
import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    
   Integer canetas;
   Double pago;
   Double troco;
    
    Scanner entrada = new Scanner(System.in);
    
    System.out.println("Quantas canetas desejas comprar?");
    canetas = entrada.nextInt();
    
    System.out.println("Quantos reais você pagará?");
    pago = entrada.nextDouble();
    
    troco = pago-2*canetas;
    System.out.println("Esse será seu troco recebido, considerando que cada caneta custa 2 reais: " + troco);
    
  }
}

QUESTAO 10
import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    
    String hora;
    Integer mints;
    Integer passaram;
    
   // TIVE DIFICULDADE, PESQUISEI E TENTEI MAS NÃO COMPREENDI A QUESTÃO
   
    Scanner entrada = new Scanner(System.in);
    System.out.println("Informe o horário em sua máquina.");
    hora = entrada.nextLine();
    
    String[]partes = hora.split(":");
    int horas = Integer.parseInt(partes[0]);
    int minutos = Integer.parseInt(partes[1]);
    
    mints = horas*60;
    int min = (horas*60) + minutos;
    passaram = mints + minutos;
    
    System.out.println("Se passaram: " + passaram);
    
  }
}

